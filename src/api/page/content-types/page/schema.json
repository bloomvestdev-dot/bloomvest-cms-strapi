{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title"
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "blocks": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "blocks.about-us",
        "blocks.our-story",
        "blocks.mission-vision",
        "blocks.our-values",
        "blocks.why-choose",
        "layout.footer",
        "blocks.education-hero",
        "blocks.latest-course",
        "blocks.featured-block",
        "blocks.blog-hero",
        "blocks.latest-article",
        "blocks.webinar-hero",
        "blocks.upcoming-webinars",
        "blocks.archived-webinars",
        "blocks.contact-hero",
        "blocks.service-hero"
      ]
    }
  }
}
